{
  "name": "@byot-backend/main",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:generate-scenarios": "cucumber-generator --output file --outputDirectory ./test/features/steps --featuresDirectory ./test/features/scenarios --relativePathToFeatures ./test/features/scenarios"
  },
  "dependencies": {
    "@hapi/joi": "17.1.1",
    "@nestjs-modules/mailer": "1.4.2",
    "@nestjs/common": "7.0.9",
    "@nestjs/config": "0.4.0",
    "@nestjs/core": "7.0.9",
    "@nestjs/graphql": "7.3.1",
    "@nestjs/jwt": "7.0.0",
    "@nestjs/passport": "7.0.0",
    "@nestjs/platform-express": "7.0.9",
    "@nestjs/typeorm": "7.0.0",
    "apollo-server-express": "2.12.0",
    "bcrypt": "4.0.1",
    "chance": "1.1.4",
    "google-auth-library": "6.0.0",
    "googleapis": "49.0.0",
    "graphql": "15.0.0",
    "graphql-tools": "5.0.0",
    "handlebars": "4.7.6",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "mysql": "2.18.1",
    "nanoid": "3.1.5",
    "nodemailer": "6.4.6",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.5.5",
    "showdown": "1.9.1",
    "typeorm": "0.2.24"
  },
  "devDependencies": {
    "@nestjs/cli": "7.1.2",
    "@nestjs/schematics": "7.0.0",
    "@nestjs/testing": "7.0.9",
    "@types/bcrypt": "3.0.0",
    "@types/chance": "1.0.10",
    "@types/express": "4.17.6",
    "@types/handlebars": "4.1.0",
    "@types/hapi__joi": "16.0.12",
    "@types/jest": "25.2.1",
    "@types/lodash": "4.14.150",
    "@types/moment": "2.13.0",
    "@types/nanoid": "2.1.0",
    "@types/node": "13.13.2",
    "@types/nodemailer": "6.4.0",
    "@types/passport-jwt": "3.0.3",
    "@types/passport-local": "1.0.33",
    "@types/showdown": "1.9.3",
    "@types/supertest": "2.0.8",
    "cucumber-generator": "1.1.9",
    "jest": "25.4.0",
    "jest-cucumber": "2.0.11",
    "prettier": "2.0.5",
    "supertest": "4.0.2",
    "ts-jest": "25.4.0",
    "ts-loader": "7.0.1",
    "ts-node": "8.9.1",
    "tsconfig-paths": "3.9.0",
    "tslint": "6.1.1",
    "typescript": "3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
